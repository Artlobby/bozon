    "ejs": "^2.5.7",
    "electron": "^4.1.4",
    "electron-builder": "^20.39.0",
    "gulp": "^4.0.0",
    "gulp-json-editor": "^2.5.1",
    "inquirer": "^6.3.1",
    "merge-config": "^2.0.0",
    "underscore.string": "^3.3.5",
    "webpack-stream": "^5.2.1"
  },
  "preferGlobal": "true",
