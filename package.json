    "gulp-json-editor": "^2.2.1",
    "inquirer": "^3.2.0",
    "merge-config": "^2.0.0",
    "underscore.string": "^3.3.4"
  },
  "preferGlobal": "true",
  "bin": {
    "bozon": "bin/bozon"
  },
  "engines": {
    "node": ">= 5.10.1"
  },
  "devDependencies": {
    "chai": "^3.5.0",
